@echo off
TITLE KakaoTalk Notification Changer
COLOR 60
>nul 2>&1 "%SYSTEMROOT%\system32\cacls.exe" "%SYSTEMROOT%\system32\config\system"
if '%errorlevel%' NEQ '0' (
    echo request UAC...
    goto UACPrompt
) else ( goto gotAdmin )
:UACPrompt
    echo Set UAC = CreateObject^("Shell.Application"^) > "%temp%\getadmin.vbs"
    set params = %*:"=""
    echo UAC.ShellExecute "cmd.exe", "/c %~s0 %params%", "", "runas", 1 >> "%temp%\getadmin.vbs"
    "%temp%\getadmin.vbs"
    rem del "%temp%\getadmin.vbs"
    exit /B
:gotAdmin
pushd "%CD%"
    CD /D "%~dp0"

cd C:\Program Files (x86)\Kakao\KakaoTalk\resource\sound

if exist "click.wav" (
    ren ns_1_01.wav bak.wav
    timeout /t 1
    ren click.wav ns_1_01.wav
    start noti_change.vbs
) else (
    ren ns_1_01.wav click.wav
    timeout /t 1
    ren bak.wav ns_1_01.wav
    start noti_reset.vbs
)	

